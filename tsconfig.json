{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "importHelpers": true,
    "moduleResolution": "Node",
    "jsx": "react",
    "declaration": true,
    "declarationDir": "./dist/types",
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src"]
}

